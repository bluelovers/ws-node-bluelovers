{
  "name": "eslint-config-bluelovers",
  "version": "1.0.14",
  "description": "my eslint config with @typescript-eslint/eslint-plugin",
  "keywords": [
    "bluelovers",
    "config",
    "eslint",
    "eslintconfig",
    "eslintrc",
    "javascript",
    "rc",
    "styleguide",
    "ts",
    "typescript"
  ],
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "coverage": "npx nyc npm run test",
    "lint": "eslint .",
    "ncu": "npx npm-check-updates -u",
    "prepublishOnly": "npm run ncu && npm run sort-package-json && node ./scripts/sort-eslintrc.js",
    "postpublish": "git commit -m \"publish new version\" .",
    "sort-package-json": "npx sort-package-json ./package.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@bluelovers/eslint-plugin": "^1.0.6",
    "@yarn-tool/find-tsconfig": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "*",
    "sort-object-keys2": "^1.1.0"
  },
  "peerDependencies": {
    "@bluelovers/eslint-plugin": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "*",
    "eslint": "*"
  },
  "preferGlobal": true
}
