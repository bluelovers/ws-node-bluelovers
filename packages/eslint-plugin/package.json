{
  "name": "@bluelovers/eslint-plugin",
  "version": "1.0.5",
  "description": "",
  "keywords": [
    "bluelovers",
    "config",
    "eslint",
    "eslint-plugin",
    "eslintconfig",
    "eslintplugin",
    "eslintrc",
    "javascript",
    "rc",
    "styleguide",
    "ts",
    "typescript"
  ],
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "coverage": "npx nyc npm run test",
    "lint": "npx eslint **/*.ts",
    "ncu": "npx yarn-tool ncu -u",
    "prepublishOnly": "npm run ncu && npm run sort-package-json && npm run test",
    "postpublish": "git commit -m \"publish new version\" .",
    "sort-package-json": "npx sort-package-json ./package.json",
    "test": "npx ts-node ./test/rule/no-irregular-whitespace"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {
    "@typescript-eslint/experimental-utils": "^1.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
