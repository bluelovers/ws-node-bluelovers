{
  "name": "@bluelovers/eslint-plugin",
  "version": "1.0.10",
  "description": "",
  "keywords": [
    "bluelovers",
    "config",
    "eslint",
    "eslint-plugin",
    "eslintconfig",
    "eslintplugin",
    "eslintrc",
    "javascript",
    "rc",
    "styleguide",
    "ts",
    "typescript"
  ],
  "homepage": "https://github.com/bluelovers/ws-node-bluelovers#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-node-bluelovers/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-node-bluelovers.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npx ts-node ./test/rule/no-irregular-whitespace",
    "preversion": "yarn run prepublishOnly",
    "prepublishOnly": "yarn run test",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "ncu": "yarn-tool ncu -u",
    "sort-package-json": "yarn-tool sort"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
